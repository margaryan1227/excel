<?xml version="1.0" encoding="UTF-8" standalone="yes"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" id="VorbehandlungEndoprotheseBasis">	<xs:include schemaLocation="../Datentypen/IRD_DatenTypen.xsd"/>	<xs:annotation>		<xs:documentation>			Dieses Schema beschreibt die Elemente des Bausteins VorbehandlungEndoprotheseBasis.			Es handelt sich um Endoprothesen-spezifische Angaben zur Vorbehandlung. 			Elemente:			- VEP_VorOpKnieSchluessel: Voroperation Knie			- VEP_VorOpHuefteSchluessel: Voroperation Hüfte			- VEP_VorOpErlaeuterung: Erläuterung für die Voroperation, falls der Schlüssel 'Andere' ausgewählt worden ist.		</xs:documentation>	</xs:annotation>	<xs:complexType name="VorbehandlungEndoprotheseBasis">		<xs:sequence>			<xs:choice minOccurs="0">				<xs:element name="VEP_VorOpKnieSchluessel" type="enum_0211"/>				<xs:element name="VEP_VorOpHuefteSchluessel" type="enum_0210"/>			</xs:choice>			<xs:element name="VEP_VorOpErlaeuterung" type="Freitext250" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="VorbehandlungEndoprotheseBasis">		<xs:sequence>			<xs:choice minOccurs="0">				<xs:element name="VEP_VorOpKnieSchluessel" type="enum_0211"/>				<xs:element name="VEP_VorOpHuefteSchluessel" type="enum_0210"/>			</xs:choice>			<xs:element name="VEP_VorOpErlaeuterung" type="Freitext250" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="VorbehandlungEndoprotheseBasis">		<xs:sequence>			<xs:choice minOccurs="0">				<xs:element name="VEP_VorOpKnieSchluessel" type="enum_0211"/>				<xs:element name="VEP_VorOpHuefteSchluessel" type="enum_0210"/>			</xs:choice>			<xs:element name="VEP_VorOpErlaeuterung" type="Freitext250" minOccurs="0"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="VorbehandlungEndoprotheseBasis">		<xs:sequence>			<xs:choice minOccurs="0">				<xs:element name="VEP_VorOpKnieSchluessel" type="enum_0211"/>				<xs:element name="VEP_VorOpHuefteSchluessel" type="enum_0210"/>			</xs:choice>			<xs:element name="VEP_VorOpErlaeuterung" type="Freitext250" minOccurs="0"/>		</xs:sequence>	</xs:complexType></xs:schema>